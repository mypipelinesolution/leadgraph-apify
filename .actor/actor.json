{
  "actorSpecification": 1,
  "name": "leadgraph",
  "title": "LeadGraphâ„¢ - Local Business Leads with AI-Powered Outreach",
  "description": "Discover local business leads with AI-generated cold emails, voicemail scripts, and SMS messages. Includes website enrichment, lead scoring, and ready-to-use outreach content.",
  "version": "0.3",
  "buildTag": "latest",
  "input": "./INPUT_SCHEMA.json",
  "output": "./output_schema.json",
  "dockerfile": "./Dockerfile",
  "readme": "./README.md",
  "defaultRunOptions": {
    "timeout": 1800
  },
  "storages": {
    "dataset": {
      "actorSpecification": 1,
      "title": "Local Business Leads with AI Outreach",
      "description": "High-quality leads with contact info, lead scores, and personalized AI-generated outreach content ready to use.",
      "views": {
        "overview": {
          "title": "Lead Overview",
          "description": "Quick view of lead quality and contact info",
          "transformation": {
            "fields": ["tier", "leadScore", "businessName", "phone", "email", "website", "city", "state", "rating", "reviewCount"]
          },
          "display": {
            "component": "table",
            "properties": {
              "tier": { "label": "Tier", "format": "text" },
              "leadScore": { "label": "Score", "format": "number" },
              "businessName": { "label": "Business", "format": "text" },
              "phone": { "label": "Phone", "format": "text" },
              "email": { "label": "Email", "format": "text" },
              "website": { "label": "Website", "format": "link" },
              "city": { "label": "City", "format": "text" },
              "state": { "label": "State", "format": "text" },
              "rating": { "label": "Rating", "format": "number" },
              "reviewCount": { "label": "Reviews", "format": "number" }
            }
          }
        },
        "outreach": {
          "title": "AI Outreach Content",
          "description": "Ready-to-use personalized outreach messages",
          "transformation": {
            "fields": ["businessName", "email", "coldEmailSubject", "coldEmailBody", "voicemailScript", "smsMessage"]
          }
        }
      }
    }
  }
}
